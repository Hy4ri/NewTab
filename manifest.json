{
  "manifest_version": 3,
  "name": "NewTab Extension",
  "version": "1.0",
  "description": "A custom new tab with clock, weather, and Speed Dial.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.open-meteo.com/*"],
  "action": {
    "default_popup": "popup.html",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
